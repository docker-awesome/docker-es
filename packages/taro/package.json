  },
  "peerDependencies": {
    "@tarojs/helper": "^3.6.32",
    "@tarojs/runtime": "^3.6.32",
    "@tarojs/shared": "^3.6.32",
    "@tarojs/taro": "^3.6.32"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
